{
  "apiVersion": "apps/v1",
  "kind": "Deployment",
  "metadata": {
    "name": "serving-deployment",
    "labels": {
      "app": "model-serving"
    }
  },
  "spec": {
    "replicas": 1,
    "selector": {
      "matchLabels": {
        "app": "model-serving"
      }
    },
    "template": {
      "metadata": {
        "labels": {
          "app": "model-serving"
        }
      },
      "spec": {
        "containers": [
          {
            "name": "model-serving",
            "image": "tomcli/gender-serving:0.1",
            "ports": [
              {
                "containerPort": 5000,
                "name": "serving"
              }
            ],
            "env": [
              {
                "name": "DUMMY",
                "value": ""
              }
            ]
          }
        ]
      }
    }
  }
}
